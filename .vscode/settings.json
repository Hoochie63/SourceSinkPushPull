{
    // if you use vscode-setup.sh, then you only need to edit factorioPath
    "factorio.versions": [
        {
            "name": "steam",
            "factorioPath": "/storage/Steam/steamapps/common/Factorio/bin/x64/factorio",
            "active": true
        }
    ],
    "Lua.workspace.library": [
        ".vscode/factorio/library",
        ".vscode/factorio-data",
        ".vscode/flib"
    ],
    "Lua.runtime.version": "Lua 5.2",
    "Lua.runtime.path": [
        "?.lua"
    ],
    "Lua.runtime.special": {
        "__object_name": "type"
    },
    "Lua.runtime.builtin": {
        "io": "disable",
        "os": "disable",
        "coroutine": "disable",
        "package": "disable",
        "math": "disable",
        "debug": "disable"
    },
    "Lua.diagnostics.globals": [
        "__DebugAdapter",
        "__Profiler"
    ],
    "Lua.diagnostics.disable": [
        "lowercase-global"
    ],
    "Lua.runtime.plugin": ".vscode/factorio/plugin.lua",
    "Lua.language.fixIndent": false
}